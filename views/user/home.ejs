        <script>

document.addEventListener("DOMContentLoaded", function() {
    const defaultValue = document.getElementById('defaultValue').value;
    console.log('asd',defaultValue);

    function setDefaultOption(selectElement, value) {
    console.log('ethypoi',selectElement,value);

        for (var i = 0; i < selectElement.options.length; i++) {
            if (selectElement.options[i].value === value) {
                selectElement.selectedIndex = i;
                break;
            }
        }
    }

    // Call the function to set the default value
    var dropdown = document.getElementById("sort-dropdown");
    setDefaultOption(dropdown, defaultValue);
});

       document.getElementById("searchForm").addEventListener("submit", function(event) {
    const searchInput = document.getElementById("searchSortOptions").value.trim();
    const sortOption = document.getElementById("sort-dropdown").value;
    const inputLetter = document.getElementById("inputLetter").value; // Get the input letter
    
    if (!searchInput) {
        // If search input is empty, set a specific value for inputStatus
        document.getElementById("inputStatus").value = "EMPTY";
    }
    
    // Set the sorting option
    document.getElementById("sortOption").value = sortOption;
    
    // Set the input letter
    document.getElementById("inputLetter").value = inputLetter;
});

document.getElementById("searchSortOptions").addEventListener("input", function(event) {
    const inputLetter = event.target.value.charAt(event.target.value.length - 1);
    console.log("Input letter:", inputLetter);
    document.getElementById("inputLetter").value = inputLetter; // Update the hidden input with the input letter
});


//Wishlist
function addToWishlist(productId) {
    console.log(productId);
    fetch('/wishlist', {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json'
        },
        body: JSON.stringify({ productId: productId })
    })
    .then(response => {
        if (response.ok) {
            return response.json(); // Parse response body as JSON
        } else {
            throw new Error('Failed to add product to wishlist');
        }
    })
    .then(data => {
        console.log(data.message); // Display success message
    })
    .catch(error => {
        console.error('Error:', error);
    });
}


    const priceSlider = document.getElementById('price-slider');
    const priceOutput = document.getElementById('price-output');

    // Function to update the output value
    function updatePriceOutput() {
        priceOutput.textContent = priceSlider.value;
    }

    // Add event listener to update output when slider value changes
    priceSlider.addEventListener('input', updatePriceOutput);

    
</script>


</script>

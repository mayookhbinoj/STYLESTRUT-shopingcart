   $(document).ready(function() {
    $("#changePasswordButton").click(function() {
        var oldPassword = $("#oldpasswordInput").val();
        var newPassword = $("#newpasswordInput").val();
        var confirmPassword = $("#confirmpasswordInput").val();

        // Check if new password and confirm password match
        if (newPassword !== confirmPassword) {
            Swal.fire({
                icon: 'error',
                title: 'Oops...',
                text: 'New password and confirm password must match.'
            });
            return;
        }

        // Send AJAX request to change password
        $.ajax({
            url: "/changepassword",
            method: "POST",
            data: {
                oldPassword: oldPassword,
                newPassword: newPassword
            },
            success: function(response) {
                console.log(response);
                // Check if the response indicates success
                if (response.success) {
                    // Display success message using Swal
                    Swal.fire({
                        icon: 'success',
                        title: 'Success!',
                        text: response.message || 'Password changed successfully.'
                        
                    });

                    // Optionally, reset form fields
                    $("#oldpasswordInput").val("");
                    $("#newpasswordInput").val("");
                    $("#confirmpasswordInput").val("");
                } else {
                    // If the response indicates failure, display the error message
                    Swal.fire({
                        icon: 'error',
                        title: 'Oops...',
                        text: response.message || 'Failed to change password.'
                    });
                }
            },
            error: function(xhr, status, error) {
                // Display error message using Swal
                Swal.fire({
                    icon: 'error',
                    title: 'Oops...',
                    text: 'Failed to change password. Please try again later.'
                });
                console.error(error);
            }
        });
    });
});


const addressForm = document.getElementById("addressForm");

addressForm.addEventListener("submit", function(event) {
    event.preventDefault(); // Prevent default form submission

    // Validate street address
    const streetInput = document.getElementById("street");
    const streetError = document.getElementById("streetError");
    if (streetInput.value.trim() === "") {
        streetError.textContent = "Street address is required";
        streetInput.classList.add("is-invalid");
        return;
    } else {
        streetError.textContent = "";
        streetInput.classList.remove("is-invalid");
    }

    // Validate ZIP code (example validation)
    const zipCodeInput = document.getElementById("zipCode");
    const zipCodeError = document.getElementById("zipCodeError");
    if (!/^\d{6}$/.test(zipCodeInput.value.trim())) {
        zipCodeError.textContent = "Invalid ZIP code";
        zipCodeInput.classList.add("is-invalid");
        return;
    } else {
        zipCodeError.textContent = "";
        zipCodeInput.classList.remove("is-invalid");
    }

    // If all validations pass, submit the form
    this.submit();
});

document.getElementById("toggleFormLink").addEventListener("click", function(event) {
        event.preventDefault(); // Prevent the default behavior of the link
        var form = document.getElementById("addressForm");
        if (form.style.display === "none") {
            form.style.display = "block"; // Show the form
        } else {
            form.style.display = "none"; // Hide the form
        }
    });
    


    document.getElementById('openReturnReasonModal').addEventListener('click', function() {
    const orderId = document.getElementById('orderId').value;
    console.log(orderId);
   
    Swal.fire({
        title: 'Are you sure you want to return the product?',
        icon: 'warning',
        showCancelButton: true,
        confirmButtonText: 'Yes',
        cancelButtonText: 'No',
        confirmButtonColor: '#3085d6',
        cancelButtonColor: '#d33',
        preConfirm: (result) => {
            if (result) {
                // If user clicks "Yes", show dropdown select menu for reason selection
                Swal.fire({

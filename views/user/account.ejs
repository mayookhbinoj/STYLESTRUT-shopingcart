                            <option value="itemNotDescribed">Item not as described</option>
                            <option value="wrongItemReceived">Wrong item received</option>
                            <option value="itemDamaged">Item damaged</option>
                        </select>
                    `,
                    icon: 'info',
                    showCancelButton: true,
                    confirmButtonText: 'Submit',
                    cancelButtonText: 'Cancel',
                    confirmButtonColor: '#3085d6',
                    cancelButtonColor: '#d33'
                }).then((result) => {
                    if (result.isConfirmed) {
                        // Send AJAX request to submit return
                        fetch(`/submitReturn?id=${orderId}`, { method: 'PATCH' })
                        .then(response => {
                            if (!response.ok) {
                                throw new Error('Failed to return the product');
                            }
                            return response.json();
                        })
                        .then(data => {
                            console.log(data); // Log response data for debugging
                            if (data.success) {
                                Swal.fire('Return Successful', 'The product has been returned.', 'success');
                                window.location.reload(true);
                            } else {
                                throw new Error('Failed to return the product');
                            }
                        })
                        .catch(error => {
                            console.error(error);
                            Swal.fire('Error!', 'Failed to return the product.', 'error');
                        });
                    }
                });
            }
        }
    });
});

      
